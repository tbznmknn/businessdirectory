{
  "targets": {
    "prisma:generate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "prisma generate --schema=libs/database/prisma/schema.prisma"
      }
    },
    "prisma:migrate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "prisma migrate dev --schema=libs/database/prisma/schema.prisma"
      }
    },
    "prisma:push": {
      "executor": "nx:run-commands",
      "options": {
        "command": "prisma db push --schema=libs/database/prisma/schema.prisma"
      }
    },
    "prisma:studio": {
      "executor": "nx:run-commands",
      "options": {
        "command": "prisma studio --schema=libs/database/prisma/schema.prisma"
      }
    }
  }
}
